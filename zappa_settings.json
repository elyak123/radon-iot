{
    "dev": {
        "django_settings": "radon.settings",
        "project_name": "radon",
        "runtime": "python3.7",
        "s3_bucket": "ZAPPA_BUCKET",
        "aws_region": "AWS_REGION_DEPLOYMENT",
        "domain": "DOMAIN_NAME",
        "certificate_arn" :"SSL_CERT",
        "events": [{
            "function": "radon.views.lectura",
            "event_source": {
                "arn": "AWS_SNS_ARN",
                    "events": ["sns:Publish"]
            }
        }],
        "exclude": [".env"],
        "keep_warm": false
    }
}
